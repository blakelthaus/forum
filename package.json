    "lodash": "^4.17.5",
    "popper.js": "^1.12",
    "postcss": "^8.2.12",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.15.2",
    "sass-loader": "^7.1.0",
    "tailwindcss": "^2.1.1",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.6.12"
  },
